<div class="element">
  <h2 class="title">Create A New Community</h2>

  <%= form_for @community do |f| %>
    <p><%= f.label :name %><br />
      <br />
    <%= f.text_field :name, :size=>"80" %></p>
    <br />

    <p><%= f.label :header %><br />
      <br />
    <%= f.text_field :header, :size=>"80" %></p>
      <br />

    <p class="submit"><%= f.submit %></p>
  <% end %>

</div>
