<h2>Edit Your Profile</h2>
<%= form_for @user do |f| %>
  <p><%= f.label :email %><br />
  <%= f.text_field :email %></p>

  <p><%= f.label :name %><br />
  <%= f.text_field :name %></p>

  <p><%= f.label :age %><br />
  <%= f.text_field :age %></p>

  <p><%= f.label :image %><br />
  <%= f.text_field :image %></p>

  <%= f.submit "Submit" %>
<% end %>
